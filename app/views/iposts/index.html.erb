<style>
  #page-header{
    height:320px;
  }
</style>
<div class="page-header">


<div id="static-pagination">
  <% will_paginate %>
</div>

<div id="my-posts">
  <div class="page-delimiter first-page" data-page="<%= params[:page] || 1 %>"></div>
  <%= render @iposts %>
</div>

<% if @iposts.next_page %>
    <div id="infinite-scrolling">
      <%= will_paginate %>
    </div>
<% end %>

</div>
