<%= form_for(@mini_post_comment_likings) do |f| %>
  <% if @mini_post_comment_likings.errors.any? %>
    <div  id="login-messages" class="ui-widget">
		<div class="ui-state-error ui-corner-all" style="padding: 0 .7em;">
			<p align="center"><span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
			<strong class="error-counter"><%= pluralize(@mini_post_comment_likings.errors.count, t("error")) %> :</strong> </p>
			 <div id="error-list">
				 <% @mini_post_comment_likings.errors.full_messages.each do |msg| %>
			        <li><%= msg %></li>
			      <% end %>
			      <br />
		      </div>
		</div>
	</div>
	<br />
  <% end %>

   <div class="field">
    <%= f.label t(:user) %><% current_user.id %><br />
    <%= f.text_field :user_id %>
  </div><br>
  <div class="field">
    <%= f.label t(:mini_post_comment_id) %><br />
    <%= f.text_field :mini_post_comment_id %>
  </div><br>
  <div class="field">
    <%= f.label t(:liking) %><br />
   
  </div><br>
  <div class="field">
    <%= f.label t(:liking) %><br />
    <%= f.submit "like" ,:name=>"like",:type=>:image,:src => "/images/like.png" %>
    <%= f.submit "dislike",:name=>"dislike",:type=>:image,:src => "/images/dislike.png" %>
   </div><br>
   <div class="field">
    <%= f.label t(:created_at) %><br />
    <%= f.text_field :created_at %>
  </div>
  <br>
<% end %>
